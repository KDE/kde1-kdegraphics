FILES = kfax-1.html kfax-2.html kfax-3.html kfax-4.html kfax-5.html kfax-6.html kfax.html 

DOCDIR = @prefix@/share/doc/HTML/kfax
INSTALL_DIR = @INSTALL@ -d -m 0755
INSTALL_DATA = @INSTALL@ -m 644

all:

install:
	$(INSTALL_DIR) $(DOCDIR)
	@for i in $(FILES); do \
	 echo "$(INSTALL_DATA) $$i $(DOCDIR)"; \
	 $(INSTALL_DATA) "$$i" $(DOCDIR); \
	done

uninstall:
	for i in $(FILES); do \
	 rm -f $(DOCDIR)/$$i ; \
	done
