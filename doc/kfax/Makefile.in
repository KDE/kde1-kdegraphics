prefix = @prefix@

FILES = index-1.html index-2.html index-3.html index-4.html index-5.html index-6.html index.html 

DOCDIR = $(prefix)/share/doc/HTML/en/kfax
INSTALL_DIR = @INSTALL@ -d -m 0755
INSTALL_DATA = @INSTALL@ -m 644

all:

install:
	$(INSTALL_DIR) $(DOCDIR)
	@for i in $(FILES); do \
	 echo "$(INSTALL_DATA) $$i $(DOCDIR)"; \
	 $(INSTALL_DATA) "$$i" $(DOCDIR); \
	done

uninstall:
	for i in $(FILES); do \
	 rm -f $(DOCDIR)/$$i ; \
	done
