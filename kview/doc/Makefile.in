FILES =  index.html kview.gif toolbar1.xpm toolbar2.xpm

DOCDIR = @prefix@/share/doc/HTML/en/kview
INSTALL_DIR = @INSTALL@ -d -m 0755
INSTALL_DATA = @INSTALL@ -m 644

all:

install:
	$(INSTALL_DIR) $(DOCDIR)
	@for i in $(FILES); do \
	 echo "$(INSTALL_DATA) $$i $(DOCDIR)"; \
	 $(INSTALL_DATA) "$$i" $(DOCDIR); \
	done

uninstall:
	for i in $(FILES); do \
	 rm -f $(DOCDIR)/$$i ; \
	done
