version.o xcalloc.o xfopen.o xmalloc.o xopendir.o xputenv.o xrealloc.o   \
xstat.o xstrdup.o selfdir.o dirname.o $(malloc)

library = kpathsea

default all: $(library).a

$(library).a: $(objects)
	rm -f $@
	$(AR) $(ARFLAGS) $@ $(objects)
	$(RANLIB) $@

